{
  "name": "dude-wheres-my-module",
  "version": "0.0.0-development",
  "description": "JavaScript suggested import server",
  "main": "index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src test --cache",
    "lint:fix": "eslint --fix src test --cache",
    "lint:watch": "esw --watch src test --cache",
    "prettier": "prettier --write src/**/*.js test/**/*.js",
    "prettier:check": "prettier --list-different src/**/*.js test/**/*.js",
    "flow": "flow",
    "flow:coverage": "for file in src/**.js test/**.js; do echo $file; flow coverage $file; done",
    "flow:watch": "flow-watch -e js,js.flow,flowconfig --ignore es/ --ignore node_modules/ --watch .flowconfig --watch src/ --watch test/",
    "clean": "rimraf es lib $(cd src; ls) *.js.flow",
    "build": "npm run clean && babel src --out-dir es --ignore '**/__tests__/**' && flow-copy-source -v src/ es --ignore '**/__tests__/**' && cross-env BABEL_ENV=es5 babel src --out-dir . --ignore '**/__tests__/**' && flow-copy-source -v src/ . --ignore '**/__tests__/**'",
    "test": "cross-env NODE_ENV=production BABEL_ENV=es5 DWMM_TEST=1 mocha --exit $npm_package_config_mocha && cross-env NODE_ENV=production BABEL_ENV=coverage DWMM_TEST=1 nyc --reporter=lcov --reporter=text mocha --exit $npm_package_config_mocha",
    "test:watch": "cross-env NODE_ENV=production BABEL_ENV=test DWMM_TEST=1 mocha --watch $npm_package_config_mocha",
    "test:debug": "cross-env NODE_ENV=production BABEL_ENV=test DWMM_TEST=1 mocha --inspect-brk $npm_package_config_mocha",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov; codecov",
    "prepublishOnly": "npm run clean && npm run prettier:check && npm run lint && flow && npm test && npm run build",
    "open:coverage": "open coverage/lcov-report/index.html",
    "semantic-release": "semantic-release"
  },
  "bin": {
    "dwmm": "cli.js",
    "dude": "cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint && flow",
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@jedwards1211/commitlint-config"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "config": {
    "mocha": "-r @babel/register test/configure.js src/__tests__/*.js src/**/__tests__/*.js",
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jedwards1211/dude-wheres-my-module.git"
  },
  "keywords": [
    "es2015"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jedwards1211/dude-wheres-my-module/issues"
  },
  "homepage": "https://github.com/jedwards1211/dude-wheres-my-module#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-1": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-conventional": "^6.0.2",
    "@jedwards1211/commitlint-config": "^1.0.0",
    "@jedwards1211/eslint-config": "^2.0.0",
    "@jedwards1211/eslint-config-flow": "^1.0.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "codecov": "^3.1.0",
    "copy": "^0.3.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-watch": "^4.0.2",
    "flow-bin": "^0.92.0",
    "flow-copy-source": "^2.0.2",
    "flow-watch": "^1.1.4",
    "husky": "^1.1.4",
    "istanbul": "^0.4.5",
    "lint-staged": "^8.0.4",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.2",
    "prettier-eslint": "^8.8.2",
    "recast": "^0.16.1",
    "rimraf": "^2.6.0",
    "semantic-release": "^15.13.24"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "@jcoreio/typed-event-emitter": "^1.0.0",
    "JSONStream": "^1.3.5",
    "ansi-escapes": "^3.1.0",
    "ast-types": "^0.11.7",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "delay": "^4.1.0",
    "find-root": "^1.1.0",
    "flow-parser": "^0.86.0",
    "fs-extra": "^7.0.1",
    "glob-gitignore": "^1.0.11",
    "jscodeshift": "^0.5.1",
    "lockfile": "^1.0.4",
    "lodash": "^4.17.11",
    "p-event": "^2.1.0",
    "semver": "^6.3.0",
    "touch": "^3.1.0",
    "yargs": "^14.0.0"
  },
  "renovate": {
    "extends": [
      ":separateMajorReleases",
      ":combinePatchMinorReleases",
      ":ignoreUnstable",
      ":prImmediately",
      ":renovatePrefix",
      ":updateNotScheduled",
      ":preserveSemverRanges",
      ":semanticPrefixFixDepsChoreOthers",
      ":automergeDisabled",
      "group:monorepos"
    ],
    "automerge": true,
    "major": {
      "automerge": false
    }
  }
}
